        .animationImg {
            animation-duration: 4s;
            animation-iteration-count: infinite;
            animation-name: bounce-1;
            transform-origin: bottom;
            animation-timing-function: linear;
        }

        @@keyframes bounce-1 {
            0% {
                transform: translateY(-35px);
            }

            50% {
                transform: translateY(30px);
            }

            100% {
                transform: translateY(-35px);
            }
        }
    </style>

